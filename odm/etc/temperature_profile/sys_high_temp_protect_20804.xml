    <ToleranceSecondStepIn>540</ToleranceSecondStepIn>
    <ToleranceSecondStepOut>520</ToleranceSecondStepOut>
    <ToleranceThreshold>110</ToleranceThreshold>
    <ExceptTemperature>460</ExceptTemperature>
    <ToleranceStart>430</ToleranceStart>
    <ToleranceStop>410</ToleranceStop>
    <ToleranceInterval>30000</ToleranceInterval>
    <HighTemperatureControlVideoRecordSwitch>true</HighTemperatureControlVideoRecordSwitch>
    <HighTemperatureRestoreBrightnessLimit>475</HighTemperatureRestoreBrightnessLimit>
</filter-conf>
